;; shadow-cljs configuration
{:source-paths
 ["src/dev"
  "src/main"
  "src/test"]

 :dependencies
   [[reagent "0.8.1"]
   [cljs-http "0.1.45"]]

 :builds
 {:frontend
  {:target :browser
   :output-dir "public/js"
   :asset-path "js"
   :modules {:main {:init-fn sartar.frontend.app/init}}
   :devtools
   {:before-load sartar.frontend.app/stop
    :after-load sartar.frontend.app/start
    :http-root "public"
    :http-port 8080}
   }}}
